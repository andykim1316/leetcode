        size = len(nums)
    
        L = [1]*size
        R = [1]*size
        final = [1]*size

        for i in range (1, size):
            L[i] = L[i-1] * nums[i-1]

        for i in range (size - 2, -1, -1):
            R[i] = R[i+1] * nums[i+1]

