class Solution(object):
    def productExceptSelf(self, nums):
     
        final = []
        product = 1
    
        for i in range (len(nums)):
            product *= nums[i]
    
        for i in range (len(nums)):
            final.append(product)
        
        for i in range (len(nums)):
            final[i] = final[i]/nums[i]
    
        return final
    

        
