class Solution(object):
    def productExceptSelf(self, nums):
     
        size = len(nums)
        ans = [1]*size
        right = 1
        
        for i in range (1, size):
            ans[i] = ans[i-1] * nums[i-1]
            
        for i in range (size - 1, -1, -1):
            ans[i] = ans[i] * right
            right *= nums[i]
        
        return ans
        
